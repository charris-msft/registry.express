{
  "$schema": "https://static.modelcontextprotocol.io/schemas/2025-10-17/server.schema.json",
  "name": "com.microsoft/azure",
  "title": "Azure MCP",
  "description": "All Azure MCP tools to create a seamless connection between AI agents and Azure services.",
  "version": "2.0.0-beta.6",
  "icons": [
    {
      "src": "https://azure.microsoft.com/svghandler/azure/?width=48&height=48",
      "mimeType": "image/png",
      "sizes": ["48x48"]
    }
  ],
  "repository": {
    "url": "https://github.com/Azure/azure-mcp",
    "source": "github"
  },
  "websiteUrl": "https://aka.ms/azmcp",
  "packages": [
    {
      "registryType": "npm",
      "identifier": "@azure/mcp",
      "version": "2.0.0-beta.6",
      "runtimeHint": "npx",
      "transport": {
        "type": "stdio"
      },
      "packageArguments": [
        {
          "value": "server",
          "type": "positional"
        },
        {
          "value": "start",
          "type": "positional"
        }
      ]
    }
  ]
}